{
    "name": "Xplained Pro SAM C21N",
    "target": "xplained_c21n",
    "cc": "armgcc",
    "classes": [
        "xplained_c21n.XplainedProSamC21N"
    ],
    "ram": 32,
    "flash": 256,
    "family_type": "armcmx",
    "family_name": "atmelsamc21n",
    "chip_model": "SAMC21N",
    "manual_reset": 0,
    "preferred_baud": 115200,
    "board_producer": "Atmel",
    "chip_manifacturer": "Atmel",
    "architecture": "cortex-m0plus",
    "gccopts": {
        "arch": [
            "cortex-m0plus",
            "-mthumb"
        ],
        "cflags": [
            "-mcpu=cortex-m0plus",
            "-mthumb",
            "-O2",
            "-fomit-frame-pointer",
            "-falign-functions=16",
            "-ffunction-sections",
            "-fdata-sections",
            "-fno-common",
            "-Wall",
            "-Wextra",
            "-Wstrict-prototypes",
            "-nostdlib",
            "-std=c99"
        ],
        "defs": [
            "__ATSAMC21N18A__",
            "SAMC21_VHAL",
            "VM_DEFAULT_THREAD_SIZE=1024"
        ]
    },
    "relocator": "cortex-m",
    "type": "board",
    "virtualizable": "XplainedProSamC21N",
    "uplink_reset": "reset",
    "reset_time": 2000,
    "vm_mini_chunk": 64,
    "has_alter_ego": false,
    "connection": {
        "baudrate": 115200,
        "parity": "n",
        "bytesize": 8,
        "stopbits": 1,
        "dsrdtr": false,
        "rtscts": false
    },
    "crypto_provisioning": "lightweight"
}
